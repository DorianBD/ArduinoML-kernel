app RedButton initial state a {
	bricks
		Actuator red_led : 8
		Actuator buzzer : 9

		Sensor button : 10

	states
		a {
			red_led <= LOW
			buzzer <= LOW
			button is HIGH => b
		}
		b {
			red_led <= LOW
			buzzer <= HIGH
			button is HIGH => c
		}
		c
        {
            red_led <= HIGH
            buzzer <= LOW
            button is HIGH => a
        }
}

