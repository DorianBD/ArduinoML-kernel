app RedButton initial state off {
	bricks
		Actuator buzzer : 9

		Sensor button1 : 10
	    Sensor button2 : 11


	states
		off {
			buzzer <= LOW
			( after 200 AND ( button1 is HIGH AND button2 is HIGH )) => on
		}
		on {
			buzzer <= HIGH
			NOT ( button1 is LOW OR button2 is LOW )  => off
		}
}

